<%= render(Primer::BaseComponent.new(tag: :nav, **@system_arguments)) do %>
  <nav-list>
    <% groups.each_with_index do |group, index| %>
      <% if index > 0 %>
        <%= render(Primer::Alpha::ActionList::Divider.new) %>
      <% end %>
      <%= group %>
    <% end %>
  </nav-list>
<% end %>
