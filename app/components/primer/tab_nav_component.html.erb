<%= wrapper do %>
  <%= render Primer::BaseComponent.new(**@system_arguments) do %>
    <%= render Primer::BaseComponent.new(**@body_arguments) do %>
      <% tabs.each do |tab| %>
        <%= tab %>
      <% end %>
    <% end %>
  <% end %>

  <% if @with_panel %>
    <% tabs.each do |tab| %>
      <%= tab.panel %>
    <% end %>
  <% end %>
<% end %>
