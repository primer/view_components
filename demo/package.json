{
  "name": "demo",
  "private": true,
  "version": "0.1.0",
  "dependencies": {
    "@primer/css": "^20.4.4",
    "@primer/primitives": "^7.9.0",
    "@rails/actioncable": "^7.0.3-1",
    "@rails/ujs": "^7.0.3-1",
    "@storybook/addon-a11y": "^v6.5.10",
    "@storybook/addon-actions": "^v6.5.10",
    "@storybook/addon-controls": "^v6.5.10",
    "@storybook/addon-docs": "^v6.5.10",
    "@storybook/server": "^v6.5.10",
    "postcss": "^8.4.12",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.4.1",
    "react-syntax-highlighter": "^15.4.4",
    "turbolinks": "^5.2.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "scripts": {
    "dev": "bin/dev",
    "build:css": "yarn --cwd ../ run build:css && postcss ./app/assets/stylesheets/application.postcss.css -o ./app/assets/builds/application.css",
    "build:ts": "yarn --cwd ../ run tsc",
    "build:rollup": "yarn --cwd ../ run rollup -c",
    "storybook": "start-storybook -p 5000",
    "build-storybook": "build-storybook -o public/view-components/stories --preview-url=/view-components/stories/iframe.html --force-build-preview",
    "build": "yarn build-storybook && yarn build:css && yarn build:ts && yarn build:rollup"
  },
  "engines": {
    "yarn": "1.22.x"
  }
}
